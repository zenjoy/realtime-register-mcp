# RealtimeRegister MCP Server Configuration
# Copy this file to .env and fill in your actual values

# Required: Your RealtimeRegister API key for authentication
REALTIME_REGISTER_API_KEY=your_api_key_here

# Optional: RealtimeRegister API base URL 
# Production (default): https://api.yoursrs.com
# Test environment: https://api.yoursrs-ote.com
REALTIME_REGISTER_BASE_URL=https://api.yoursrs.com

# Optional: Request timeout in milliseconds (defaults to 30000)
REALTIME_REGISTER_TIMEOUT=30000

# Optional: Enable debug logging (defaults to false)
REALTIME_REGISTER_DEBUG=false

# Optional: Logging level - error, warn, info, debug (defaults to info)
LOG_LEVEL=info
