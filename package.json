{
  "name": "realtime-register-mcp",
  "version": "0.1.2",
  "description": "Model Context Protocol server for RealtimeRegister domain management operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:local": "node test-local.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepare": "yarn build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "realtime-register",
    "domain",
    "dns",
    "ssl"
  ],
  "author": "engineering@zenjoy.be",
  "license": "MIT",
  "bin": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenjoy/realtime-register-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/zenjoy/realtime-register-mcp/issues"
  },
  "homepage": "https://github.com/zenjoy/realtime-register-mcp#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@realtimeregister/api": "^1.0.0",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.12.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@jest/globals": "30.0.0-beta.3",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
